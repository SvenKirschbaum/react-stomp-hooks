{
  "name": "react-stomp-hooks",
  "version": "2.1.0",
  "description": "A react library to interact with a stomp connection via hooks",
  "author": "fallobst22",
  "license": "MIT",
  "repository": "fallobst22/react-stomp-hooks",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --format modern,cjs --no-compress --jsx React.createElement",
    "start": "microbundle watch --format modern,cjs --no-compress --jsx React.createElement",
    "test": "run-s test:unit test:lint",
    "test:lint": "eslint --ext .tsx,.ts .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": ">16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.4.12",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/sockjs-client": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^3.2.3",
    "microbundle": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@stomp/stompjs": "^6.1.2",
    "sockjs-client": "^1.6.0"
  },
  "files": [
    "dist"
  ]
}
